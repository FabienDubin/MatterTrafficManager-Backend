  "dependencies": {
    "@notionhq/client": "^2.2.15",
    "@types/bull": "^3.15.9",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "axios": "^1.7.0",
    "bcrypt": "^6.0.0",
    "bottleneck": "^2.19.5",
    "bull": "^4.16.5",
    "cors": "^2.8.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.4.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.18.0",
