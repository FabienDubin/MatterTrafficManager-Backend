{
  "name": "matter-traffic-backend",
  "version": "1.0.0",
  "description": "API backend pour Matter Traffic Manager",
  "main": "src/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "lint-staged": "lint-staged",
    "seed:admin": "ts-node src/scripts/seed-test-users.ts",
    "db:init": "ts-node src/scripts/init-db.ts",
    "db:create-indexes": "ts-node src/scripts/create-indexes.ts",
    "db:seed": "ts-node src/scripts/seed-db.ts",
    "notion:update-config": "ts-node src/scripts/update-notion-config-prod.ts",
    "redis:clear": "ts-node src/scripts/clear-redis-cache.ts",
    "test:redis": "ts-node src/scripts/test-redis.ts",
    "test:notion-data": "ts-node src/scripts/test-notion-data.ts",
    "test:webhook-monitoring": "ts-node src/scripts/monitor-webhook-redis.ts",
    "test:batch-resolver": "ts-node src/scripts/test-batch-resolver.ts",
    "test:multi-teams": "ts-node src/scripts/test-multi-teams.ts",
    "test:calendar-endpoint": "ts-node src/scripts/test-calendar-endpoint.ts",
    "webhook:capture": "ts-node src/scripts/azure-webhook-capture.ts",
    "webhook:capture:local": "ts-node src/scripts/capture-webhook-token.ts",
    "import:colors": "ts-node src/scripts/import-client-colors.ts",
    "import:configs": "ts-node src/scripts/init-all-configs.ts",
    "diagnose:calendar": "ts-node src/scripts/diagnose-prod-calendar.ts",
    "docker:rebuild:backend": "npm run build && docker-compose build backend && docker-compose up -d backend",
    "docker:rebuild:frontend": "cd ../matter-traffic-frontend && npm run build && cd ../matter-traffic-backend && docker-compose build frontend && docker-compose up -d frontend",
    "docker:rebuild:all": "npm run docker:rebuild:backend && npm run docker:rebuild:frontend"
  },
  "dependencies": {
    "@notionhq/client": "^2.2.15",
    "@types/node-cron": "^3.0.11",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@upstash/redis": "^1.35.3",
    "axios": "^1.7.0",
    "bcrypt": "^6.0.0",
    "bottleneck": "^2.19.5",
    "cors": "^2.8.0",
    "dataloader": "^2.2.3",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.4.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.18.0",
    "node-cron": "^4.2.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.15.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.0",
    "@types/express": "^5.0.0",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^29.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.0.0",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "husky": "^9.0.0",
    "jest": "^29.0.0",
    "lint-staged": "^15.0.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.0.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.7.0"
  },
  "keywords": [
    "notion",
    "traffic",
    "management",
    "api"
  ],
  "author": "FabLab",
  "license": "MIT",
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  }
}
